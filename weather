		var temp_chart;
		function fnd_chart_temp(maxtemp, mintemp, date) {
			//max = Math.max(...maxtemp);
			//min = Math.min(...mintemp);
			max = 0;
			min = -273;
			$.each(maxtemp, function() {
				if (this>max)
					max = this;
			});
			$.each(mintemp, function() {
				if (this<min || min == -273)
					min = this;
			});

			max = max%2>0?max+3:max+2;
			min -= 4;
			temp_chart = new Highcharts.Chart({
				chart: {
					renderTo: 'container_7day_chart_temp',
					backgroundColor: {
						linearGradient: {
							x1: 0,
							y1: 0,
							x2: 0,
							y2: 1
						},
						stops: [
							[0, 'rgb(255, 255, 255)'],
							[1, 'rgb(252, 255, 197)']
						]
					},
					//width: 100,
					height: 350
				},
				colors: ['#BE4B48', '#4A7EBB'],
				legend: {
					enabled: false
				},
				title: {
					text: 'Temperature (°C)',
					y: 20,
					style: {
						color: '#E66300',
						fontSize: '20px',
						fontWeight: 'bold'
					}
				},
				credits: {
					enabled: false
				},
				xAxis: {
					crosshair: {
						width: 1,
						color: "#d3d3d3"
					},
					labels: {
						enabled: true,
						style: {
							fontSize: '16px'
						}
					},
					categories: date
				},
				yAxis: {
					maxPadding: 0.5,
					minPadding: 0.5,
					tickInterval: 2,
					gridLineColor: '#d3d3d3',
					max: max,
					min: min,
					labels: {
						enabled: false
					},
					title: null
				},
				tooltip: {
					enabled: false
				},
				plotOptions: {
					line: {
						dataLabels: {
							enabled: true,
							borderRadius: 5,
							borderWidth: 1,
							y: -16,
							borderColor: "#AAA",
							borderRadius: 5,
							style: {
								fontWeight: 'bold',
								fontSize: '12pt'
							}
						},
						shadow: true,
						marker: {
							enabled: true
						}
					},
					arearange: {
						cursor: 'pointer',
						events: {
							click: function(event) {
								if (this.color == "#BE4B48") {
									window.location = lang_root+"probfcst/tempfcst.htm#max"
								} else if (this.color == "#4A7EBB") {
									window.location = lang_root+"probfcst/tempfcst.htm#min"
								}
							}
						},
						marker: {
							enabled: false,
							states: {
								hover: {
									enabled: false
								}
							}
						},
						states: {
							hover: false
						},
						lineWidth: 0,
						fillOpacity: 0.2,
						zIndex: 0,
						animation: false
					}
				},
				series: [{
						type: 'line',
						name: 'Forecasted Highest',
						marker: {
							symbol: 'square'
						},
						color: "#BE4B48",
						dataLabels: {
							backgroundColor: '#FFDB69',
							y: 0
						},
						data: maxtemp
					},
					{
						type: 'line',
						name: 'Forecasted Lowest',
						marker: {
							symbol: 'circle'
						},
						color: "#4A7EBB",
						dataLabels: {
							enabled: true,
							backgroundColor: '#C6D9F1',
							y: +40
						},
						data: mintemp
					},
					{
						name: 'Max Temp Rang:',
						data: [
							[climatData.climat.day1.maxTemp_25per, climatData.climat.day1.maxTemp_75per],
							[climatData.climat.day2.maxTemp_25per, climatData.climat.day2.maxTemp_75per],
							[climatData.climat.day3.maxTemp_25per, climatData.climat.day3.maxTemp_75per],
							[climatData.climat.day4.maxTemp_25per, climatData.climat.day4.maxTemp_75per],
							[climatData.climat.day5.maxTemp_25per, climatData.climat.day5.maxTemp_75per],
							[climatData.climat.day6.maxTemp_25per, climatData.climat.day6.maxTemp_75per],
							[climatData.climat.day7.maxTemp_25per, climatData.climat.day7.maxTemp_75per],
							[climatData.climat.day8.maxTemp_25per, climatData.climat.day8.maxTemp_75per],
							[climatData.climat.day9.maxTemp_25per, climatData.climat.day9.maxTemp_75per]
						],
						type: 'arearange'
					},
					{
						name: 'Min Temp Rang:',
						data: [
							[climatData.climat.day1.minTemp_25per, climatData.climat.day1.minTemp_75per],
							[climatData.climat.day2.minTemp_25per, climatData.climat.day2.minTemp_75per],
							[climatData.climat.day3.minTemp_25per, climatData.climat.day3.minTemp_75per],
							[climatData.climat.day4.minTemp_25per, climatData.climat.day4.minTemp_75per],
							[climatData.climat.day5.minTemp_25per, climatData.climat.day5.minTemp_75per],
							[climatData.climat.day6.minTemp_25per, climatData.climat.day6.minTemp_75per],
							[climatData.climat.day7.minTemp_25per, climatData.climat.day7.minTemp_75per],
