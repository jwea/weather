		function init_left_side_height(){
		
			$('#new_fnd>div').each(function(){
				var left_div = $(this).find('>div:first-child>div:last-child').outerHeight();
				var right_div = $(this).find('>div:last-child').outerHeight();
				
				if(left_div <= right_div && window.innerWidth > 700){
					$(this).find('>div:first-child>div:last-child').css('min-height', right_div);
				}else{
					$(this).find('>div:first-child>div:last-child').css('min-height', 0);
				}
			});
		}
		
					//20200514 switch weather icon 
		var cookie_icon_name_fnd_local = "HKO_DefaultWeatherIcon";
		
		function initWxIcon() { 
			if ($.cookie(cookie_icon_name_fnd_local) == "nwxicon") {
				changeHomepageWxIconSetting("nwxicon");
			}else if ($.cookie(cookie_icon_name_fnd_local) == "owxicon") {
				changeHomepageWxIconSetting("owxicon");				
			}else{
				changeHomepageWxIconSetting("nwxicon");
			}
		}
		
		function toggleWxIconSetting() { 
			if ($.cookie(cookie_icon_name_fnd_local) == "nwxicon") {
				changeHomepageWxIconSetting("owxicon");
			}else if ($.cookie(cookie_icon_name_fnd_local) == "owxicon") {
				changeHomepageWxIconSetting("nwxicon");				
			}else{
				changeHomepageWxIconSetting("owxicon");
			}
		}
		
		function changeHomepageWxIconSetting(theme) { 
			/***save cookie value here ***/
			$.cookie(cookie_icon_name_fnd_local, theme, {
				expires: 365,
				path: "/"
			});
			
			if ($.cookie(cookie_icon_name_fnd_local) == "owxicon") {
				console.log("owxicon");
				$("#old_fnd .old_fnd_upper img").each(function() {
					var src = $(this).attr('src').replace('../../../images/HKOWxIconOutline/','/content_elements_v2/images/weather-icon-70x70/');
					$(this).attr('src', src);
				});
				$("#new_fnd .new_fnd_item div div img").each(function() {
					var src = $(this).attr('src').replace('../../../images/HKOWxIconOutline/','/content_elements_v2/images/weather-icon-70x70/');
					$(this).attr('src', src);
				}); 
				
				
				$("#btnSwitchIcon").attr("src", "/content_elements_v2/images/weather-icon-70x70/pic50.png");
				$("#btnSwitchIcon").css("padding", "0px");
			}else if($.cookie(cookie_icon_name_fnd_local) == "nwxicon"){
				console.log("nwxicon");
				$("#old_fnd .old_fnd_upper img").each(function() {
					var src = $(this).attr('src').replace('/content_elements_v2/images/weather-icon-70x70/', '../../../images/HKOWxIconOutline/');
					$(this).attr('src', src);
				}); 
				$("#new_fnd .new_fnd_item div div img").each(function() {
					var src = $(this).attr('src').replace('/content_elements_v2/images/weather-icon-70x70/', '../../../images/HKOWxIconOutline/');
					$(this).attr('src', src);
				}); 
				$("#btnSwitchIcon").attr("src", "/images/HKOWxIconOutline/pic50.png");
				$("#btnSwitchIcon").css("padding", "2px");
			}
		}
		//end 20200514 switch weather icon


		$(window).resize(function(){
			init_left_side_height();
		});
	</script>
