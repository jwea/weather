    <script type="text/javascript">
		
var menu_id = '0';
var section_id = '2018010100000010786';

                    function chglang(l){
	// var currentURL=document.location.href;
       var currentURL = location.protocol + '//' + location.host + location.pathname
      
	var langArr = [
					['en','index.html'],
					['tc','index.html'],
					['sc','index.html']
					];
	var nextURL = '';
	if(currentURL.indexOf("/"+langArr[lang][0]+"/")>=0){
		//nextURL=currentURL.ReplaceAll("/"+langArr[lang][0]+"/","/"+langArr[l][0]+"/");
		nextURL=currentURL.replace("/"+langArr[lang][0]+"/","/"+langArr[l][0]+"/");
	}else if(currentURL.indexOf(langArr[lang][1])>=0){
		//nextURL=currentURL.ReplaceAll("/"+langArr[lang][1],"/"+langArr[l][1]);
		nextURL=currentURL.replace("/"+langArr[lang][1],"/"+langArr[l][1]);
	}else{
		nextURL=currentURL+langArr[l][1];
	}
	//document.location.href=nextURL.replace(/#.*/,'');
	document.location.href=nextURL;
        console.log("local chglang");

}                       


        
function jump_to_notes(){
				$(document).scrollTop( $("#tablenote").offset().top - 220 );  
			}


           $(window).on('load', function() {
                        if (window.location.href.indexOf("tablenote") > -1) {
                           $(document).scrollTop( $("#tablenote").offset().top -130);  
                        }  
               });

                                                                                                                                                                                
var rh_chart;
		function fnd_chart_RH(maxrh, minrh, date) {
			max = 0;
			min = -1;
			
			$.each(maxrh, function() {
				if (this>max)
					max = this;
			});
			$.each(minrh, function() {
				if (this<min || min == -1)
					min = this;
			});

			max = max%2>0?max+4:max+3;
			min -= 7;
			
			if (max > 100) {
				max = 100;
			}
			
			if (min < 0) {
				min = 0;
			}
			
			rh_chart = new Highcharts.Chart({
				chart: {
					renderTo: 'container_7day_chart_rh',
					backgroundColor: {
						linearGradient: {
							x1: 0,
							y1: 0,
							x2: 0,
							y2: 0.5
						},
						stops: [
							[0, 'rgba(255, 255, 255, 1)'],
							[1, '#D5FFEC']
						]
					},
					//width: 100%,
					height: 350
				},
				colors: ['#BE4B48', '#4A7EBB'],
				legend: {
					enabled: false
				},
				title: {
					text: 'Humidity (%)',
					y: 20,
					style: {
						color: '#0092D6',
						fontSize: '20px',
						fontWeight: 'bold'
					}
				},
				credits: {
					enabled: false
				},
				xAxis: {
					crosshair: {
						width: 1,
						color: "#d3d3d3"
					},
					labels: {
						enabled: true,
						style: {
							fontSize: '16px'
						}
					},
					categories: date
				},
				yAxis: {
					maxPadding: 0.5,
					minPadding: 0.5,
					tickInterval: 10,
					max: max,
					min: min,
					labels: {
						enabled: false
					},
					title: null,
					gridLineColor: '#d3d3d3'
				},
				tooltip: {
					enabled: false
				},
				plotOptions: {
					line: {
						dataLabels: {
							enabled: true,
							borderRadius: 5,
							borderWidth: 1,
							style: {
								fontWeight: 'bold',
								fontSize: '12pt'
							}
						},
						shadow: true
					},
					arearange: {
						marker: {
							enabled: false,
							states: {
								hover: {
									enabled: false
								}
							}
						},
						states: {
							hover: false
						},
						lineWidth: 0,
						fillOpacity: 0.2,
						animation: false
					}
				},
				series: [{
						name: 'Highest',
						marker: {
							symbol: 'square'
						},
						dataLabels: {
							backgroundColor: '#FFDB69',
							y: -5
						},
						data: maxrh
					},
					{
						name: 'Lowest',
						marker: {
							symbol: 'circle'
						},
						dataLabels: {
							backgroundColor: '#C6D9F1',
							y: +40
						},
						data: minrh
					},
					{
						name: 'Max Temp Rang:',
						data: [
							[climatData.climat.day1.maxRH_25per, climatData.climat.day1.maxRH_75per],
							[climatData.climat.day2.maxRH_25per, climatData.climat.day2.maxRH_75per],
							[climatData.climat.day3.maxRH_25per, climatData.climat.day3.maxRH_75per],
							[climatData.climat.day4.maxRH_25per, climatData.climat.day4.maxRH_75per],
							[climatData.climat.day5.maxRH_25per, climatData.climat.day5.maxRH_75per],
							[climatData.climat.day6.maxRH_25per, climatData.climat.day6.maxRH_75per],
							[climatData.climat.day7.maxRH_25per, climatData.climat.day7.maxRH_75per],
							[climatData.climat.day8.maxRH_25per, climatData.climat.day8.maxRH_75per],
							[climatData.climat.day9.maxRH_25per, climatData.climat.day9.maxRH_75per]
						],
						type: 'arearange'
					},
					{
						name: 'Min Temp Rang:',
						data: [
							[climatData.climat.day1.minRH_25per, climatData.climat.day1.minRH_75per],
							[climatData.climat.day2.minRH_25per, climatData.climat.day2.minRH_75per],
							[climatData.climat.day3.minRH_25per, climatData.climat.day3.minRH_75per],
							[climatData.climat.day4.minRH_25per, climatData.climat.day4.minRH_75per],
							[climatData.climat.day5.minRH_25per, climatData.climat.day5.minRH_75per],
							[climatData.climat.day6.minRH_25per, climatData.climat.day6.minRH_75per],
							[climatData.climat.day7.minRH_25per, climatData.climat.day7.minRH_75per],
							[climatData.climat.day8.minRH_25per, climatData.climat.day8.minRH_75per],
							[climatData.climat.day9.minRH_25per, climatData.climat.day9.minRH_75per]
						],
						type: 'arearange'
					}
				]
			});
			rh_chart2 = new Highcharts.Chart({
				chart: {
					renderTo: 'container_7day_chart_rh2',
					backgroundColor: {
						linearGradient: {
							x1: 0,
